	ЕСТЬNULL(ВложенныйЗапрос.Кузовщик, ЗНАЧЕНИЕ(Справочник.Scan_Контрагенты.ПустаяСсылка)),
	ЕСТЬNULL(ВложенныйЗапрос.ЗаказНаЗакупку, ЗНАЧЕНИЕ(Документ.Scan_ЗаказНаЗакупку.ПустаяСсылка)),
