	ОбластьЗаголовок.Параметры.ДатаДокумента = Формат(ДокументСсылка.ДатаРаспоряжения, "ДЛФ=DD");
	ТабличныйДокумент.Вывести(ОбластьЗаголовок);
	//Шапка таблицы
	ОбластьШапка = Макет.ПолучитьОбласть("ОбластьШапка");
	ТабличныйДокумент.Вывести(ОбластьШапка);
	
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Дилер КАК Дилер,
	|	Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Менеджер КАК Менеджер,
	|	Scan_КомпанииРегионыОтветственности.Регион КАК Регион,
	|	Scan_КомпанииРегионыОтветственности.Регион.ФедеральныйОкруг КАК ФедеральныйОкруг,
	|	Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Дилер.Наименование КАК ДилерНаименование
	|ИЗ
	|	Документ.Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерами.ОтветственныеМенеджеры КАК Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры
	|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Scan_Компании.РегионыОтветственности КАК Scan_КомпанииРегионыОтветственности
	|		ПО Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Дилер = Scan_КомпанииРегионыОтветственности.Ссылка
	|ГДЕ
	|	Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Ссылка = &СсылкаДокумент
	|
	|УПОРЯДОЧИТЬ ПО
	//rarus bonmak 29.03.2021 17306 ++
	//|	ФедеральныйОкруг,
	//|	Регион,
	//|	ДилерНаименование,
	//|	Менеджер
	//|	Менеджер,
	//|	Регион,
	//|	ДилерНаименование
	|  Scan_КомпанииРегионыОтветственности.Регион.ФедеральныйОкруг.Наименование,
	|  Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Менеджер.Наименование,
	|  Scan_КомпанииРегионыОтветственности.Регион.Наименование,
	|  Scan_ЗакреплениеМенеджеровДепартаментаПродажАвтобусовЗаДилерамиОтветственныеМенеджеры.Дилер.Наименование
	//rarus bonmak 29.03.2021 17306 --
	|ИТОГИ ПО
	|	ФедеральныйОкруг";
	
	Запрос.Параметры.Вставить("СсылкаДокумент", ДокументСсылка);
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаФО = РезультатЗапроса.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "ФедеральныйОкруг");
	ОбъединениеНачало = 8;
	ОбъединениеКонец = 8;
	НомерСтраницы = 1;
	Пока ВыборкаФО.Следующий() Цикл
		ОбластьСтрокаФО = Макет.ПолучитьОбласть("СтрокаФО");
		ОбластьСтрокаФО.Параметры.ЗначениеФО = ВыборкаФО.ФедеральныйОкруг;
		НомерСтраницы = Scan_Печать.ВывестиГоризонтальнуюОбласть(ТабличныйДокумент, ОбластьСтрокаФО, , , НомерСтраницы, , ДокументСсылка);
		
		ВыборкаДетЗаписи = ВыборкаФО.Выбрать();
		Пока ВыборкаДетЗаписи.Следующий() Цикл
			
			ОбластьСтрокаРегионы = Макет.ПолучитьОбласть("СтрокаРегионы");
			ОбластьСтрокаРегионы.Параметры.ЗначениеМенеджер = ВыборкаДетЗаписи.Менеджер;
			ОбластьСтрокаРегионы.Параметры.ЗначениеРегион = ВыборкаДетЗаписи.Регион;
			ОбластьСтрокаРегионы.Параметры.ЗначениеДилер = ВыборкаДетЗаписи.Дилер;
			НомерСтраницы = Scan_Печать.ВывестиГоризонтальнуюОбласть(ТабличныйДокумент, ОбластьСтрокаРегионы, , , НомерСтраницы, , ДокументСсылка);
			ОбъединениеКонец = ОбъединениеКонец + 1;
		КонецЦикла;
		//объединим одинаковые ячейки
		ОбъединениеНачалоМенеджер = ОбъединениеНачало;
		ОбъединениеНачалоРегион   = ОбъединениеНачало;
		ОбъединениеНачалоДилер    = ОбъединениеНачало;
		Для Строка = ОбъединениеНачало По ОбъединениеКонец Цикл
			//объединение Регион
			Если ТабличныйДокумент.Область("R"+Строка+"C2").Текст = ТабличныйДокумент.Область("R"+Строка(Строка-1)+"C2").Текст
				ИЛИ ОбъединениеНачалоРегион = Строка Тогда ;
			Иначе
				ТабличныйДокумент.Область("R"+ОбъединениеНачалоРегион+"C2"+":"+"R"+Строка(Строка-1)+"C2").Объединить();
				ОбъединениеНачалоРегион = Строка;
			КонецЕсли;
			//объединение Дилер
			Если ТабличныйДокумент.Область("R"+Строка+"C3").Текст = ТабличныйДокумент.Область("R"+Строка(Строка-1)+"C3").Текст
				ИЛИ ОбъединениеНачалоДилер = Строка Тогда ;
			Иначе
				ТабличныйДокумент.Область("R"+ОбъединениеНачалоДилер+"C3"+":"+"R"+Строка(Строка-1)+"C3").Объединить();
				ОбъединениеНачалоДилер = Строка;
			КонецЕсли;
			//объединение Менеджер
			Если ТабличныйДокумент.Область("R"+Строка+"C4").Текст = ТабличныйДокумент.Область("R"+Строка(Строка-1)+"C4").Текст
				ИЛИ ОбъединениеНачалоМенеджер = Строка Тогда ;
			Иначе
				ТабличныйДокумент.Область("R"+ОбъединениеНачалоМенеджер+"C4"+":"+"R"+Строка(Строка-1)+"C4").Объединить();
				ОбъединениеНачалоМенеджер = Строка;
			КонецЕсли;
			
		КонецЦикла;
		ОбъединениеНачало = ОбъединениеКонец + 1; 
		ОбъединениеКонец  = ОбъединениеКонец + 1;
	КонецЦикла;
	
	ОбластьПодвал = Макет.ПолучитьОбласть("ОбластьПодвал");
	ОбластьПодвал.Параметры.ПодписантДолжность = ДокументСсылка.ПодписантВПФ.Должность;
	ОбластьПодвал.Параметры.Подписант = ДокументСсылка.ПодписантВПФ;
	НомерСтраницы = Scan_Печать.ВывестиГоризонтальнуюОбласть(ТабличныйДокумент, ОбластьПодвал, , , НомерСтраницы, , ДокументСсылка);
	
	Возврат ТабличныйДокумент;
	
